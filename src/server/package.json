{
  "name": "mercuryback",
  "version": "0.0.2",
  "author": "CodeDoesGood",
  "description": "The supporting backend of the Mercury project",
  "scripts": {
    "cover": "nyc ./node_modules/mocha/bin/mocha --exit ./dist/tests/index.test.js",
    "cover:full": "nyc --reporter=html ./node_modules/mocha/bin/mocha --exit ./dist/tests/index.test.js",
    "build": "npm run build:ts && npm run lint",
    "build:ts": "tsc",
    "build:watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && node ./dist/index.js",
    "start:debug": "node --inspect ./dist/index.js",
    "start:pm2": "pm2 ./dist/index.js",
    "test": "node ./node_modules/mocha/bin/mocha --exit ./dist/tests/index.test.js",
    "test:travis": "SET TRAVIS=true && node ./node_modules/mocha/bin/mocha --exit ./dist/tests/index.test.js"
  },
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^3.15.0",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "express": "4.16.2",
    "express-rate-limit": "2.9.0",
    "jsonwebtoken": "8.1.0",
    "knex": "0.14.2",
    "lodash": "4.17.4",
    "mysql": "2.15.0",
    "nodemailer": "4.3.1",
    "pm2": "^2.9.1",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/bluebird": "^3.5.18",
    "@types/body-parser": "^1.16.8",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^2.0.20",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.0.35",
    "@types/express-rate-limit": "^2.9.3",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/knex": "0.0.67",
    "@types/lodash": "^4.14.80",
    "@types/mocha": "^2.2.44",
    "@types/mysql": "^2.15.2",
    "@types/node": "^7.0.12",
    "@types/nodemailer": "^1.3.32",
    "@types/winston": "^2.3.7",
    "babel-eslint": "8.0.3",
    "dotenv": "^4.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.4.0"
  }
}
