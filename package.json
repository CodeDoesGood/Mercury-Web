{
  "name": "mercuryback",
  "version": "0.0.4",
  "description": "The supporting backend of the mercury project",
  "main": "src/index.ts",
  "directories": {
    "doc": "docs",
    "test": "src/test",
    "build": "build"
  },
  "scripts": {
    "build": "npm run build:ts && npm run tslint",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && node ./build/index.js",
    "start:debug": "node --inspect ./build/index.js",
    "test:unit": "node ./node_modules/mocha/bin/mocha --exit ./build/test/unit/index.test.js",
    "test:travis": "SET TRAVIS=true && node ./node_modules/mocha/bin/mocha --exit ./build/test/unit/index.test.js"
  },
  "author": "CodeDoesGood",
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^3.15.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-rate-limit": "^2.11.0",
    "jsonwebtoken": "^8.1.0",
    "knex": "^0.14.2",
    "lodash": "^4.17.4",
    "mysql": "^2.15.0",
    "nodemailer": "^4.4.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.0",
    "@types/express-rate-limit": "^2.9.3",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/knex": "^0.14.0",
    "@types/lodash": "^4.14.92",
    "@types/mocha": "^2.2.46",
    "@types/mysql": "^2.15.2",
    "@types/node": "^9.3.0",
    "@types/nodemailer": "^4.3.1",
    "@types/winston": "^2.3.7",
    "mocha": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2"
  }
}
