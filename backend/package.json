    "mysql": "^2.14.1",
    "nodemailer": "^4.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.5.0",
    "nyc": "^11.2.1"
